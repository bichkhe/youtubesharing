  "description": "PostCSS preset for Mantine (7.0+) applications",
  "main": "dist/preset.js",
  "types": "dist/types/preset.d.ts",
  "peerDependencies": {
    "postcss": ">=8.0.0"
  },
  "dependencies": {
    "postcss-mixins": "^9.0.4",
